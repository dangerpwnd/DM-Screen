		{
			"name": "/login",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "70864e17-7dd2-4757-b48f-bb125a14e815",
						"exec": [
							"pm.test(\"Access token was not empty\", function() {",
							"    let jsonData = pm.response.json();",
							"    pm.expect(jsonData.access_token).not.equal(undefined);",
							"    pm.environment.set(\"jwt_token\", jsonData.access_token);",
							"});",
							"",
							"pm.test(\"Refresh token was not empty\", function() {",
							"    let jsonData = pm.response.json();",
							"    pm.expect(jsonData.refresh_token).not.equal(undefined);",
							"    pm.environment.set(\"refresh_token\", jsonData.refresh_token);",
							"})",
							""
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
